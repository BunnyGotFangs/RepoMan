{
  "name": "mui-theme",
  "version": "0.1.4",
  "description": "Theme generator cli tool for Element",
  "bin": {
    "mui-theme": "./bin/element-theme",
    "mt": "./bin/element-theme"
  },
  "files": [
    "bin",
    "lib"
  ],
  "main": "index.js",
  "scripts": {
    "test": "xo",
	"pub": "npm publish"
  },
  "xo": {
    "space": true,
    "rule": {
      "semi": 0,
      "unicorn/no-process-exit": 0,
      "import/no-dynamic-require": 0
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ElementUI/element-theme.git"
  },
  "keywords": [
    "element-ui",
    "vue",
    "theme",
    "cli"
  ],
  "bugs": {
    "url": "https://github.com/ElementUI/element-theme/issues"
  },
  "author": "qingwei-li <cinwell.li@gmail.com> (https://github.com/QingWei-Li)",
  "engines": {
    "node": ">=0.12.0"
  },
  "license": "MIT",
  "peerDependencies": {
    "mui-theme-default": "*"
  },
  "dependencies": {
    "commander": "^2.9.0",
    "gulp": "^3.9.1",
    "gulp-cssmin": "^0.1.7",
    "gulp-nop": "0.0.3",
    "gulp-postcss": "^6.2.0",
    "ora": "^0.3.0",
    "postcss-salad": "^1.0.6",
    "run-sequence": "^1.2.2",
    "through2": "^2.0.3"
  },
  "devDependencies": {
    "mui-theme-default": "*",
    "xo": "^0.17.0"
  }
}
